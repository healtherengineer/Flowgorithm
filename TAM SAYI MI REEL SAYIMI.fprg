                    <then/>
                    <else>
                        <assign variable="a" expression="1"/>
                    </else>
                </if>
            </for>
            <if expression="a=0">
                <then>
                    <output expression="&quot;TAM SAY&#304;&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;REEL SAY&#304;&quot;" newline="True"/>
                </else>
            </if>
        </body>
