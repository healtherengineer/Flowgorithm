    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="a, n, k, i, t" type="Real" array="False" size=""/>
            <assign variable="a" expression="0"/>
            <output expression="&quot;Lutfen sayiyi giriniz : &quot;" newline="True"/>
            <input variable="n"/>
            <output expression="&quot;Virgulden sonra kac sayi var onu giriniz : &quot;" newline="True"/>
            <input variable="k"/>
